extends ../includes/_layout

block content
  - var isMain = false;
  include ../includes/_header

  block main
    main.center
      .p-portfolio
        .container
          .breadcrumbs
            ul.breadcrumbs__list.flex.jc-fs.fw-w.ai-s
              li
                a.nuxt-link-active(href='/') ART MARKS
              li
                | Портфолио
          .p-services__top
            .caption.caption--big.caption--services--visible
              .caption__inner.above-viewport(data-in-viewport='i9')
                p.caption__main Портфолио
                //
                .caption__content
                  p
                    | Любой может сделать из простого сложное. Мы умеем превратить сложное в простое. За 2 года мы создали более 300 приносящих прибыль проектов, каждый из которых был сделан внимательно, тщательно и с любовью.
          .p-portfolio__filters.flex.fw-w.ai-s.jc-c
            .select.p-portfolio__select.portfolio-direction
              select
                option(selected disabled) Направление работы
                option WEB разработка
                option SMM продвижение
                option SEO продвижение
            .select.p-portfolio__select
              select
                option(selected disabled) Тип проекта
                option Интернет магазин
                option Портал

          .p-portfolio__content
            .p-portfolio__wrapper.flex.fw-w.jc-sb.ai-s
              .s-portfolio__item.p-portfolio__item
                a.above-viewport(href='/portfolio/karree', data-in-viewport='i10')
                  .s-portfolio__item-overlay
                  .s-portfolio__item-img(style='background-image:url(/img/portfolio-1.jpg);')
                  .s-portfolio__item-inner.flex.fd-c.ai-s.jc-c
                    p.s-portfolio__item-title karree
                    .s-portfolio__item-info
                      p.s-portfolio__item-description интернет-магазин женской одежды
                      p.s-portfolio__item-category web
              .s-portfolio__item.p-portfolio__item
                a.above-viewport(href='/portfolio/lisoped', data-in-viewport='i11')
                  .s-portfolio__item-overlay
                  .s-portfolio__item-img(style='background-image:url(/img/portfolio-2.jpg);')
                  .s-portfolio__item-inner.flex.fd-c.ai-s.jc-c
                    p.s-portfolio__item-title lisoped
                    .s-portfolio__item-info
                      p.s-portfolio__item-description интернет-магазин женской одежды
                      p.s-portfolio__item-category web
              .s-portfolio__item.p-portfolio__item
                a.above-viewport(href='/portfolio/pets', data-in-viewport='i12')
                  .s-portfolio__item-overlay
                  .s-portfolio__item-img(style='background-image:url(/img/portfolio-3.jpg);')
                  .s-portfolio__item-inner.flex.fd-c.ai-s.jc-c
                    p.s-portfolio__item-title 4pets
                    .s-portfolio__item-info
                      p.s-portfolio__item-description интернет-магазин женской одежды
                      p.s-portfolio__item-category web
              .s-portfolio__item.p-portfolio__item
                a.above-viewport(href='/portfolio/mshop', data-in-viewport='i13')
                  .s-portfolio__item-overlay
                  .s-portfolio__item-img(style='background-image:url(/img/portfolio-4.jpg);')
                  .s-portfolio__item-inner.flex.fd-c.ai-s.jc-c
                    p.s-portfolio__item-title m1
                    .s-portfolio__item-info
                      p.s-portfolio__item-description интернет-магазин одежды
                      p.s-portfolio__item-category web
              //
              //
              //
            .btn__wrap
              //
              button.btn.p-portfolio__btn.btn--filled(type='submit')
                .btn__wrap-inner
                  span.btn__wrap-text
                    | ПОКАЗАТЬ ЕЩЕ
                  //
        section.s-discuss.p-services-category__discuss
          .container
            .s-discuss__wrapper
              form.form.discuss__project-form.s-discuss__form
                .caption.caption--centered.caption--services--visible
                  .caption__inner.above-viewport(data-in-viewport='i14')
                    p.caption__main обсудить проект
                    p.caption__additional Все крутые проекты начинаются с заполнения этой формы
                    //
                .form__inner.flex.ai-s.jc-sb
                  .form__inner-part
                    .form-input
                      .form-input__inner
                        //
                        input(placeholder='Имя', required='required', name='username', type='text', value='')
                        i.form-input__line
                        //
                      span.form-message
                    .form-input
                      .form-input__inner
                        //
